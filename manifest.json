{
    "name" :"typewritter",
    "version" :"1.2",
    "description" :"Enable sound. Type words on every website. Have fun.",
    "homepage_url": "http://jib√©barth.fr",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon64.png"
    },
    "content_scripts": [
        {
            "js": [ "jquery.min.js", "script.js" ],
            "matches": [ "http://*/*", "https://*/*"]
        }
    ],
    "browser_action": {
        "name": "TypeWritter",
        "icons": ["icon48.png"],
        "default_icon": "icon48.png"
    },
    "web_accessible_resources": [
        "sound/space.mp3",
        "sound/space-new.mp3",
        "sound/backspace.mp3",
        "sound/return.mp3",
        "sound/return-new.mp3",
        "sound/key-01.mp3",
        "sound/key-02.mp3",
        "sound/key-03.mp3",
        "sound/key-04.mp3",
        "sound/key-new-01.mp3",
        "sound/key-new-02.mp3",
        "sound/key-new-03.mp3",
        "sound/key-new-04.mp3",
        "sound/key-new-05.mp3"
    ],
    "manifest_version": 2
}
